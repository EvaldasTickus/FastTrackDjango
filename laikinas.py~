number_list = ["1", "2", "3"]
print(number_list[:1])


def do_something(x,y:int =6):
    print(f"Hello: {x + y}")

do_something(9)

class Car:
    def __init__(self, color, model, year):
        self.color = color
        self.model = model
        self.year = year

class Auto(Car):
    def __init__(self, color, model, year):
        super().__init__(color, model, year)

numbers = list(range(1, 100))

print(numbers)
print(numbers[1::2])
print(list(num for num in numbers if num % 2 == 0))